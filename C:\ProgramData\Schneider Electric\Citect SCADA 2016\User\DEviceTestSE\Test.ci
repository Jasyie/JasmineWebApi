
FUNCTION test()
INT i,nRc;
STRING sLine;
nRc = DevOpen("Syslog", 0);
TraceMsg("++++ open handle = " + IntToStr(nRc));
WHILE 1<> 2 DO
	sLine = TimeToStr(TimeCurrent(),5);
	i = DevWriteLn(nRc,sLine);
	Beep(1);
	IF i <> 0 THEN
		Message("", "RC is:" + i:#,48);
	END
	SleepMS(2000);
END
TraceMsg("++++ close handle = " + IntToStr(nRc));
DevClose(nRC);

END

