INT fh 
FUNCTION ClientStart()
	fh = DevOpen("Syslog", 0);
	DebugMsg("++++ open handle = " + IntToStr(fh));
END

FUNCTION LogTest()
INT i,nRc;
STRING sLine;
	sLine = TimeToStr(TimeCurrent(),5);
	i = DevWriteLn(fh,sLine);
	IF i <> 0 THEN
		DebugMsg("RC IS: " + IntToStr(i));
	END
END